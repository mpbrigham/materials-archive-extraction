{
  "name": "Materials Intake Full Flow",
  "nodes": [
    {
      "parameters": {
        "protocol": "IMAP",
        "host": "imap.example.com",
        "port": 993,
        "secure": true,
        "mailbox": "INBOX",
        "downloadAttachments": true,
        "attachmentPrefix": "attachment_",
        "options": {
          "allowUnauthorizedCerts": false,
          "ignoreTextEncoding": false
        }
      },
      "id": "1",
      "name": "IMAP Email Trigger",
      "type": "n8n-nodes-base.emailReadImap",
      "typeVersion": 1,
      "position": [
        100,
        100
      ]
    },
    {
      "parameters": {
        "options": {
          "extract": "text"
        }
      },
      "id": "2",
      "name": "Extract From PDF",
      "type": "n8n-nodes-base.extractFromFile",
      "typeVersion": 1,
      "position": [
        300,
        100
      ]
    },
    {
      "parameters": {
        "functionCode": "// Supervisor Agent logic goes here\nreturn items;"
      },
      "id": "3",
      "name": "Supervisor Agent",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        500,
        100
      ]
    },
    {
      "parameters": {
        "functionCode": "// Check required fields before sending to verifier\nreturn items;"
      },
      "id": "4",
      "name": "Check Required Fields",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        700,
        100
      ]
    },
    {
      "parameters": {
        "functionCode": "// Call Metadata Extractor LLM API\nreturn items;"
      },
      "id": "5",
      "name": "Metadata Extractor",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        700,
        250
      ]
    },
    {
      "parameters": {
        "functionCode": "// Verifier Agent logic goes here\nreturn items;"
      },
      "id": "6",
      "name": "Verifier Agent",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        900,
        100
      ]
    },
    {
      "parameters": {
        "functionCode": "// Format success email\nreturn items;"
      },
      "id": "7",
      "name": "Format Success Email",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        1100,
        100
      ]
    },
    {
      "parameters": {
        "functionCode": "// Format error email\nreturn items;"
      },
      "id": "8",
      "name": "Format Error Email",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        1100,
        250
      ]
    },
    {
      "parameters": {
        "fromEmail": "materials-intake@example.com",
        "toEmail": "={{ $json[\"from\"] }}",
        "subject": "={{ $json[\"subject\"] }}",
        "text": "={{ $json[\"body\"] }}"
      },
      "id": "9",
      "name": "Send Email",
      "type": "n8n-nodes-base.emailSend",
      "typeVersion": 1,
      "position": [
        1300,
        100
      ]
    }
  ],
  "connections": {
    "IMAP Email Trigger": {
      "main": [
        [
          "Extract From PDF"
        ]
      ]
    },
    "Extract From PDF": {
      "main": [
        [
          "Supervisor Agent"
        ]
      ]
    },
    "Supervisor Agent": {
      "main": [
        [
          "Check Required Fields"
        ]
      ]
    },
    "Check Required Fields": {
      "main": [
        [
          "Verifier Agent"
        ]
      ]
    },
    "Verifier Agent": {
      "main": [
        [
          "Format Success Email"
        ]
      ]
    },
    "Format Success Email": {
      "main": [
        [
          "Send Email"
        ]
      ]
    },
    "Metadata Extractor": {
      "main": [
        [
          "Check Required Fields"
        ]
      ]
    }
  }
}